{
  "name": "ASPNetCoreReact",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production",
    "build:watch": "webpack --progress --watch"
  },
  "jest": {
    "setupFiles": [
      "./wwwroot/test/shim.js",
      "./wwwroot/test/setup.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 85,
        "lines": 90,
        "statements": 90
      }
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/wwwroot/test/__mocks__/styleMock.js",
      "ui/(.*)": "<rootDir>/wwwroot/$1",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "test/.*.js$",
      ".*(.css|.less|.scss)"
    ],
    "transform": {
      "\\.(css|scss)$": "jest-css-modules-transform",
      ".*": "babel-jest"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-module-resolver": "^3.1.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "bootstrap": "^4.0.0-beta.2",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^24.7.1",
    "jest-css-modules-transform": "^2.1.1",
    "jquery": "^3.4.0",
    "popper.js": "^1.15.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  },
  "dependencies": {
    "es6-promise": "^4.2.6"
  }
}
